#include <iostream>
#include <cmath>
using namespace std;
// 0.7 * (2 * c - 9 * k) - 4 * (0.25 * c - k)
// 0.7 * (2 * c - 9 * k) - (c - 4 * k)
//0.4 * c - 2.3 * k
int main()
{
    double c, k, a, b;
    cout <<  "Enter numbers for c and k: " << "\n";
    cin >> c >> k;
    cout << "Answer: " << "\n" << 0.4 * c - 2.3 * k ;
    return 0;
}
