#include <iostream>
//scamer ==> 1 day == 1000rub 

using namespace std;
int main(){
    double x, y;
    int count = 0;
    cin >> x >> y;
    
    while (x <= y){
        count ++;
        x *= 1.1;
    }
    
    cout << "В какой день ваше небо станет в клеточку: " << count;
    
    
    
    
    
    
    return 0;
}



