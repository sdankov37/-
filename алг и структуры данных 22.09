arr = [1, 2, 3, 4, 5, 6, 7]
    
def binary_search(list, key):
    low = 0
    high = len(list) -1
    
    while low <= high:
        mid = (low + high) // 2
        midVal = list[mid]
        
        if midVal == key:
            return mid
            
        if midVal > key:
            high = mid - 1

        else:
            low = mid + 1
    return 'Элемент не найден'

print(binary_search(arr, ))
